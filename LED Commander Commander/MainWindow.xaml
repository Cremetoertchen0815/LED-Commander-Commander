        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LED_Commander_Commander"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button" Content="Load" HorizontalAlignment="Left" Height="23" Margin="37,38,0,0" VerticalAlignment="Top" Width="86" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="Save" HorizontalAlignment="Left" Height="23" Margin="128,38,0,0" VerticalAlignment="Top" Width="86"/>

    </Grid>
</Window>
